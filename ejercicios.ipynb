    "# Sumar digitos de un numero de manera recursiva"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "La suma de los digitos -345 es 12\n"
     ]
    }
   ],
   "source": [
    "def sumaDigitos(numero):\n",
    "    if numero == 0:\n",
    "        return 0\n",
    "    else:\n",
    "        digito = numero % 10\n",
    "        return digito + sumaDigitos(numero // 10)\n",
    "\n",
    "def principal():\n",
    "    num = int(input(\"Ingrese un número: \"))\n",
    "    if num < -9 or num > 9:\n",
    "        print(f\"La suma de los digitos {num} es {sumaDigitos(abs(num))}\")\n",
    "    else:\n",
    "        print(\"El número es de un sólo digito...\", str(num))\n",
    "\n",
    "principal()"
   ]
